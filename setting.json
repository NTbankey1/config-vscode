  // ===== FILE SETTINGS =====
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  // ===== GIT SETTINGS =====
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.untrackedChanges": "separate",
  "git.ignoreRebaseWarning": true,
  "git.replaceTagsWhenPull": true,
  // ===== SCM SETTINGS =====
  "scm.diffDecorationsGutterWidth": 2,
  // ===== TERMINAL SETTINGS - OPTIMIZED FOR ZSH =====
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 1,
  "terminal.integrated.fontFamily": "'Fira Code', 'JetBrains Mono', 'Cascadia Code', 'SF Mono', 'Monaco', 'Inconsolata', 'Roboto Mono', Consolas, 'Courier New', monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.lineHeight": 1.1,
  "terminal.integrated.letterSpacing": 0.2,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.fastScrollSensitivity": 4,
  "terminal.integrated.mouseWheelScrollSensitivity": 3,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.rightClickBehavior": "selectWord",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.confirmOnKill": "editor",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.tabs.hideCondition": "never",
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.tabs.showActions": "singleTerminalOrNarrow",
  "terminal.integrated.tabs.showActiveTerminal": "always",
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.persistentSessionReviveProcess": "onExitAndWindowClose",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.allowMnemonics": false,
  "terminal.integrated.altClickMovesCursor": true,
  "terminal.integrated.commandsToSkipShell": [
    "workbench.action.terminal.clear",
    "workbench.action.terminal.new",
    "workbench.action.terminal.split",
    "workbench.action.terminal.kill"
  ],
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.shellIntegration.decorationsEnabled": "both",
  "terminal.integrated.shellIntegration.history": 10000,
  "terminal.integrated.shellIntegration.showWelcome": false,
  "terminal.integrated.shellIntegration.suggestEnabled": true,
  "terminal.integrated.shellIntegration.autoReplies": {
    "Terminate batch job (Y/N)?": "Y",
    "Do you want to continue?": "Y",
    "Are you sure?": "Y",
    "Continue?": "Y",
    "Proceed?": "Y"
  },
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "path": "/bin/zsh",
      "args": [
        "-l"
      ],
      "icon": "terminal-bash",
      "color": "terminal.ansiGreen"
    },
    "zsh (login)": {
      "path": "/bin/zsh",
      "args": [
        "-l"
      ],
      "icon": "terminal-bash",
      "color": "terminal.ansiBlue"
    },
    "zsh (interactive)": {
      "path": "/bin/zsh",
      "args": [
        "-i"
      ],
      "icon": "terminal-bash",
      "color": "terminal.ansiYellow"
    },
    "bash": {
      "path": "/bin/bash",
      "args": [
        "-l"
      ],
      "icon": "terminal-bash",
      "color": "terminal.ansiCyan"
